{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "text": {
      "type": "string",
      "description": "Line 1\nLine 2"
    },
    "enum": {
      "type": "string",
      "enum": [
        "A",
        "B",
        "C"
      ]
    },
    "specialEnum": {
      "type": "string",
      "enum": [
        "First",
        "Second",
        "Third"
      ]
    },
    "rangeInt": {
      "type": "number",
      "minimum": 0,
      "maximum": 100
    },
    "rangeDouble": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 1.0
    },
    "nested": {
      "if": {
        "type": "object"
      },
      "else": {
        "type": "null"
      },
      "properties": {
        "list": {
          "type": "array"
        },
        "rangeFloat": {
          "type": "number"
        },
        "rangeLong": {
          "type": "number"
        }
      },
      "required": [
        "list"
      ]
    },
    "sealed": {
      "if": {
        "type": "object"
      },
      "else": {
        "type": "null"
      },
      "properties": {
        "type": {
          "type": "string"
        },
        "value": {
          "type": "object"
        }
      },
      "required": [
        "type",
        "value"
      ]
    }
  },
  "required": [
    "text",
    "enum",
    "specialEnum"
  ]
}