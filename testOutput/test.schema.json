{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "text": {
      "type": "string",
      "description": "Line 1\nLine 2"
    },
    "enum": {
      "type": "string",
      "enum": [
        "A",
        "B",
        "C"
      ]
    },
    "specialEnum": {
      "type": "string",
      "enum": [
        "First",
        "Second",
        "Third"
      ]
    },
    "rangeInt": {
      "type": "number",
      "minimum": 0,
      "maximum": 100
    },
    "rangeDouble": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 1.0
    },
    "nested": {
      "if": {
        "type": "object"
      },
      "else": {
        "type": "null"
      },
      "properties": {
        "list": {
          "type": "array"
        },
        "rangeFloat": {
          "type": "number"
        },
        "rangeLong": {
          "type": "number"
        }
      },
      "required": [
        "list"
      ]
    },
    "sealed": {
      "if": {
        "type": "object"
      },
      "else": {
        "type": "null"
      },
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "TestSealed.A",
            "TestSealed.B"
          ]
        }
      },
      "anyOf": [
        {
          "type": "object",
          "properties": {
            "type": {
              "const": "TestSealed.A"
            },
            "text": {
              "type": "string"
            }
          },
          "required": [
            "text"
          ]
        },
        {
          "type": "object",
          "properties": {
            "type": {
              "const": "TestSealed.B"
            },
            "number": {
              "type": "number"
            }
          },
          "required": [
            "number"
          ]
        }
      ]
    }
  },
  "required": [
    "text",
    "enum",
    "specialEnum"
  ]
}